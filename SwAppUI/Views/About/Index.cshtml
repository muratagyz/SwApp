            </div>

            <div class="row mt-4 px-0 justify-content-between card-container-about">
                @Html.Partial("HomeWhySorswarePartial")
            </div>

        </div>
    </div>
</div>
<div class="container my-5 ">
    <div class="row">
        <div class="col-12 about-text-2">
            <h1 class="about-title">@Html.Raw(Model.AboutUI.AboutTitle)</h1>
            <p class="about-text text-black">
                @Html.Raw(Model.AboutUI.AboutDetails)
            </p>
        </div>
    </div>
